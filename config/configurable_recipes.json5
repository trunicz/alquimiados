{
	"RecipeCategories": {
		// Enables recipes for items that can't be normally crafted in Minecraft.
		"enableNewRecipes": true,
		// Enables alternative recipes for items that can be normally crafted or gathered in Minecraft.
		"enableAlternativeRecipes": true,
		// Enables recipes for turning Rotten Flesh and Dried Kelp to Leather using Campfires and Furnaces.
		"enableFleshToLeatherRecipes": true,
		// Enables recipes for turning Slabs, Stairs, and Ladders to the block or item they were crafted from,
		"enableUndoRecipes": true,
		// Enables use of multiple items from the same category for recipes.
		"enableMiscellaneousRecipes": true,
		// Enables recipes for items from supported mods.
		"enableCollaborationRecipes": true,
		// (Requires Block Of... Mod) Enables recipes for turning Rotten Flesh Block and Dried Kelp Block to Leather Block using Campfires and Furnaces.
		"enableBlockOfRecipes": true
	},
	"NewRecipes": {
		// 6 Source Material + 1 Leather = Horse Armor
		"enableHorseArmorRecipes": true,
		// 5 Leather + 1 Iron Ingot + 2 Tripwire Hook = Saddle
		"enableSaddleRecipe": true,
		// 4 Yellow Wool + 4 Any Type of Seed + 1 Magma Cream = Sponge
		"enableSpongeRecipe": true,
		// 2 Maps + 3 Strings = Name Tag
		"enableNameTagRecipe": true,
		// 2 Emeralds + 4 Golden Ingots + 1 Nether Star = Totem of Undying
		"enableTotemOfUndyingRecipe": true,
		// 3 Any Type Of Logs + 1 Gold Block + 3 Gold Ingots
		"enableBellRecipe": true,
		// 3 Nether Quartz + 3 Prismarine Shards + 1 Prismarine = Trident
		"enableTridentRecipe": true,
		// All recipes contain Iron Ingots and Chains. Please check the Wiki for the recipes.
		"enableChainmailArmorRecipes": true,
		// 5 Strings in X shape = Cobweb
		"enableCobwebRecipe": true,
		// 6 Rabbit Hides + 2 Strings = Bundle
		"enableBundleRecipe": true
	},
	"AlternativeRecipes": {
		// 1 Dirt/Grass + Rotten Flesh + 1 Bone Meal = 3 Dirt/Grass
		"enableBonemealMultipliers": true,
		// 1 Any Sapling = 2 Sticks
		"saplingToStick": true,
		// 4 Redstone + 5 Gold Nuggets = 4 Glowstone Dust
		"enableGlowstoneDustRecipe": true,
		// 50 Seconds / 1000 Ticks - 1.5 Experience
		"rottenFleshToBeefFromSmoker": true
	},
	"FleshToLeather": {
		// 40 Seconds / 800 Ticks - 0.5 Experience
		"rottenFleshFromFurnace": true,
		// 40 Seconds / 800 Ticks - 1 Experience
		"rottenFleshFromCampfire": true,
		// 23 Seconds / 460 Ticks - 0.5 Experience
		"driedKelpFromFurnace": true,
		// 23 Seconds / 460 Ticks - 1 Experience
		"driedKelpFromCampfire": true
	},
	"UndoRecipes": {
		// 6 Slabs paced horizontally = 3 Blocks
		"slabToBlock": true,
		// 4 Stairs in a square shape = 6 Blocks
		"stairToBlock": true,
		// 3 Ladders in a horizontal or vertical row = 7 Sticks
		"ladderToStick": true
	},
	"Miscellaneous": {
		// (May not work with some other mods.) Multiple stone type blocks in Tool, Furnace, Dispenser, Dropper, Brewing Stand recipes
		"enableUniversalStone": true
	},
	"BlockOfRecipes": {
		// (Requires BlockOf...) 6 Minutes / 7200 Ticks - 0.5 Experience
		"enableRottenFleshBlockToLeatherFromFurnace": true,
		// (Requires Block Of...) 6 Minutes / 7200 Ticks - 1 Experience
		"enableRottenFleshBlockToLeatherFromCampfire": true
	}
}